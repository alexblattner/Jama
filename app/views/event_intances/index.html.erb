<p id="notice"><%= notice %></p>

<h1>Event Intances</h1>

<table>
  <thead>
    <tr>
      <th>Gamestate</th>
      <th>Level</th>
      <th>Event</th>
      <th>Progress</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_intances.each do |event_intance| %>
      <tr>
        <td><%= event_intance.gamestate_id %></td>
        <td><%= event_intance.level_id %></td>
        <td><%= event_intance.event_id %></td>
        <td><%= event_intance.progress %></td>
        <td><%= link_to 'Show', event_intance %></td>
        <td><%= link_to 'Edit', edit_event_intance_path(event_intance) %></td>
        <td><%= link_to 'Destroy', event_intance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event Intance', new_event_intance_path %>
