<div class="tab-pane active" id="1">
          <div class="gs-row-1">
            <h1>Your Games</h1>
          </div>

          <div class="gs-row-2">

            <div class="gs-row-2-col">
              <div class="gs card">
                <div class="gs card-body">
                  <%= link_to(image_tag("plussign.png", :height =>"250", :width => "250", :class=>"cancelhoverEXP"), creategame_path)%>
                </div>
              </div>
            </div>
            
            <% @self.each do |game| %>
            <div class="gs-row-2-col">
              <div class="gs card">
                <% if game.game_image.blank? %>
                  <%= image_tag("defaultgame.png", :height =>"180", :width => "260")%>
                <% else %>
                    <%= image_tag(game.game_image, :height =>"180", :width => "260")%>
                <% end %>
                <div class="gs card-body">
                  <h3 class="gs card-title"><%= game.game_name %></h3>
                  <% if game.description.blank? %>
                    <h4 class="gs card-text">No description</h4>
                  <% else %>
                    <h4 class="gs card-text"><%= game.description %></h4>
                  <% end %>
                  <% if game.start_level_id!=nil%>
                  <%= link_to "Play", initiate_url(game.id), class: "btn-card" %>
                  <%end%>
                  <%= link_to "Edit", leveldashboard_url(game.id), class: "btn-card" %>
                </div>
              </div>
            </div>
            <% end %>
          </div>
        </div>